{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "functions": {
      "src/main.ts": {
        "includeFiles": "node_modules/tesseract.js-core/tesseract-core*.wasm"
      }
    },
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "src/main.ts"
      }
    ]
  }